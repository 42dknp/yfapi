{
    "name": "42dknp/yfapi-php",
    "description": "A simple PHP package for integrating Yahoo Finace Data in your applications.",
    "type": "library",
    "author": "Dominic Kneup",
    "license": "MIT",
    "keywords": ["yahoo", "finance", "api", "financial", "markets", "securites", "stocks", "etfs", "fonds", "bonds"],
    "homepage": "https://github.com/42dknp/yfapi-php",
    "require": {
        "guzzlehttp/guzzle": "^7.7"
    },
    "autoload": {
        "psr-4": {
            "yfAPI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "yfapi\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^10.3",
        "squizlabs/php_codesniffer": "^3.7"
    }
}
